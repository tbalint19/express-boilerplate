{
  "name": "express-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "start": "cross-env NODE_ENV=development DEBUG=express-sequelize node ./bin/www",
    "prod": "cross-env NODE_ENV=production node ./bin/www",
    "format": "./node_modules/.bin/prettier --write '{src,test}/**/*'",
    "format-check": "./node_modules/.bin/prettier --check '{src,test}/**/*'",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test nyc ./node_modules/.bin/mocha test/unit/*.spec.js > ./unit-coverage.txt",
    "test-integration": "cross-env NODE_ENV=test nyc ./node_modules/.bin/mocha test/integration/*.spec.js > ./integration-coverage.txt",
    "coverage-check": "node CI/scripts/coverage.js",
    "mutate": "cross-env NODE_ENV=test ./node_modules/@stryker-mutator/core/bin/stryker run"
  },
  "nyc": {
    "reporter": [
      "text-summary"
    ]
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^3.1.0",
    "sqlite": "^3.0.3"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^2.0.2",
    "@stryker-mutator/javascript-mutator": "^2.0.2",
    "@stryker-mutator/mocha-framework": "^2.0.2",
    "@stryker-mutator/mocha-runner": "^2.0.2",
    "cross-env": "^5.0.1",
    "expect.js": "^0.3.1",
    "mocha": "^6.0.0",
    "nyc": "^14.1.1",
    "prettier": "1.18.2",
    "supertest": "^2.0.0"
  }
}
