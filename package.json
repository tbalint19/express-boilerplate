{
  "name": "express-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "start": "cross-env NODE_ENV=development DEBUG=express-sequelize node ./bin/www",
    "prod": "cross-env NODE_ENV=production node ./bin/www",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/*.spec.js"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^3.1.0",
    "sqlite": "^3.0.3"
  },
  "devDependencies": {
    "cross-env": "^5.0.1",
    "expect.js": "^0.3.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-requirejs": "^1.1.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.2",
    "mocha": "^6.0.0",
    "puppeteer": "^1.11.0",
    "supertest": "^2.0.0"
  }
}
